{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "plugins": ["react-native"],
  "rules": {
    "linebreak-style": 0,
    "react/prefer-stateless-function": 0,
    "react/forbid-prop-types": 0,
    "react/require-default-props": 0,
    "arrow-parens": 0,
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }]
  }
}
